{
	"manifest_version":2,
	"name":"ToSTE",
	"version":"1.0",
	"description":"Interprets a Terms of Service webpage",
	"icons":{
		"48":"icons/toste-48.jpg"
	},
	"permissions": [
		"activeTab"
	],
	"browser_action": {
		"default_icon": "icons/toste-32.jpg",
		"default_title": "ToSTE",
		"default_popup": "popup/toste_start.js"
	},
	"web_accessible_resources": [
		"popup/img/toste_logo.png",
		"beasts/frog.jpg",
		"beasts/turtle.jpg",
		"beasts/snake.jpg"
	]
}